{
  "name": "openapi-merge-cli",
  "version": "1.0.0",
  "description": "A cli tool for the openapi-merge library.",
  "keywords": [
    "openapi",
    "merge",
    "cli"
  ],
  "main": "dist/index",
  "author": "Robert Massaioli",
  "license": "MIT",
  "private": false,
  "homepage": "https://bitbucket.org/echo_rm/openapi-merge-cli",
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:echo_rm/openapi-merge-cli.git"
  },
  "bugs": {
    "url": "https://bitbucket.org/echo_rm/openapi-merge-cli/issues?status=new&status=open"
  },
  "devDependencies": {
    "@types/node": "^13.13.5",
    "ts-node": "^8.10.1",
    "typescript": "^3.8.3",
    "typescript-json-schema": "^0.42.0"
  },
  "dependencies": {
    "ajv": "^6.12.2",
    "commander": "^5.1.0",
    "openapi-merge": "^1.0.16"
  },
  "scripts": {
    "gen-schema": "typescript-json-schema src/data.ts Configuration --titles --refs --noExtraProps --required --strictNullChecks -o src/configuration.schema.json",
    "start": "ts-node --project ./tsconfig.json ./src/index.ts"
  }
}
